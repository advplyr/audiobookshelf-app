{
  // Starts port forwarding for Android Auto Desktop Head Unit (port 5277 for ABS).
  "version": "2.0.0",
  "tasks": [
    {
      "label": "ABS: Start Android Auto port forward",
      "type": "shell",
      "command": "\"${env:LOCALAPPDATA}\\Android\\Sdk\\platform-tools\\adb.exe\" forward tcp:5277 tcp:5277",
      "problemMatcher": [],
      "presentation": {
        "reveal": "always",
        "panel": "dedicated",
        "echo": true
      }
    }
  ]
}
